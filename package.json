{
	"name": "websyte",
	"version": "2.0.1",
	"type": "module",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "remix vite:build",
		"dev": "cross-env NODE_ENV=development remix vite:dev",
		"start": "cross-env NODE_ENV=production remix-serve ./build/server/index.js",
		"test": "playwright install --with-deps && cross-env NODE_ENV=test playwright test",
		"typecheck": "tsc",
		"biome": "biome check --write"
	},
	"dependencies": {
		"@remix-run/dev": "^2.12.0",
		"@remix-run/node": "^2.12.0",
		"@remix-run/react": "^2.12.0",
		"@remix-run/serve": "^2.12.0",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"dotenv": "^16.4.5",
		"dotenv-expand": "^11.0.6",
		"isbot": "^5.1.17",
		"remix-utils": "^7.6.0",
		"tailwind-merge": "^2.5.2",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@biomejs/biome": "~1.9.0",
		"@playwright/test": "^1.47.1",
		"@types/node": "^22.5.4",
		"@types/react": "^18.3.5",
		"@types/react-dom": "^18.3.0",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"lefthook": "^1.7.15",
		"postcss": "^8.4.47",
		"tailwindcss": "^3.4.11",
		"typescript": "^5.6.2",
		"vite": "^5.4.5",
		"vite-tsconfig-paths": "^5.0.1"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "*"
	}
